{
  "expo": {
    "name": "12-places",
    "slug": "12-places",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/places.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jenryk.great_places",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "API_HERE"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/places-adaptive.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
